  {
    "tags": {
      "allowUnknownTags": [
        "component",
        "category",
        "subcategory"
      ],
      "dictionaries": [
        "jsdoc",
        "closure"
      ]
    },
    "source": {
      "include": [
        "pages",
        "controller.js",
        "components"
      ],
      "exclude":[
        "components/custom_editor"
      ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown",
      "node_modules/better-docs/component",
      "node_modules/better-docs/category"
    ],
    "templates": {
      "better-docs": {
        "name": "JsDoc Documentation",
        "title": "DevChat",
        "navLinks": [
          {
              "label": "Github",
              "href": "https://github.com/DHBW-FN-TIT20/web-notes"
          }
        ]
      }
    },
    "opts": {
      "destination": "JsDoc",
      "recurse": true,
      "readme": "README.md",
      "template": "node_modules/better-docs"
    }
  }