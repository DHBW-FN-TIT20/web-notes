  {
    "tags": {
      "allowUnknownTags": ["component"],
      "dictionaries": [
        "jsdoc",
        "closure"
      ]
    },
    "source": {
      "include": [
        "pages",
        "controller.js",
        "components"
      ],
      "exclude":[
        "components/custom_editor"
      ],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
      "plugins/markdown",
      "node_modules/better-docs/component"
    ],
    "templates": {
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md",
      "template": "node_modules/better-docs"
    }
  }